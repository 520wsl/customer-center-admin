{
	"name": "customer-center-ui",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"audio.js": "^1.1.3",
		"axios": "^0.18.0",
		"element-ui": "^2.4.9",
		"iview": "^3.1.5",
		"js-base64": "^2.4.9",
		"js-cookie": "^2.2.0",
		"less": "^3.8.1",
		"less-loader": "^4.1.0",
		"mint-ui": "^2.2.13",
		"moment": "^2.22.2",
		"video.js": "^7.3.0",
		"vue": "^2.5.17",
		"vue-router": "^3.0.1",
		"vue-video-player": "^5.0.2",
		"vuex": "^3.0.1"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^3.1.1",
		"@vue/cli-plugin-eslint": "^3.1.1",
		"@vue/cli-service": "^3.1.1",
		"babel-eslint": "^10.0.1",
		"bootstrap": "^3.3.0",
		"eslint": "^5.8.0",
		"eslint-plugin-vue": "^5.0.0-0",
		"jquery": "^1.11.3",
		"vue-template-compiler": "^2.5.17"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": "off",
			"no-debugger": "off",
			"no-unused-vars": "off",
			"generator-star-spacing": "off",
			"vue/no-parsing-error": [
				2,
				{
					"x-invalid-end-tag": false
				}
			]
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
